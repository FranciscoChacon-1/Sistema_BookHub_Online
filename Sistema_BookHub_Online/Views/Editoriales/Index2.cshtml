@model IEnumerable<Sistema_BookHub_Online.Models.Editoriales>

@{
    ViewData["Title"] = "Index2";
    Layout = "layoutu";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ubicacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contacto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ubicacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contacto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_Editorial">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_Editorial">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_Editorial">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
